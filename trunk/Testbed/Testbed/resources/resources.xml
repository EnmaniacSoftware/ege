<?xml version="1.0" encoding="UTF-8" standalone='no'?>

<resources>

  <!-- Ripples test resources -->
  <group name='ripples-test'>

    <texture type="2d" name="background" path="images/background.jpg" mag-filter="trilinear" min-filter="trilinear" />
    <material name='background' >
      <texture name='background' />
    </material>

  </group>

  <!-- Curves test resources -->
  <group name='curves-test'>

    <curve name='bezier-1' type='bezier'>
      <point pos='100 100 0' tangent='300 100 0' />
      <point pos='200 200 0' tangent='0 200 0' />
    </curve>
    
    <curve name='bezier-2' type='bezier'>
      <point pos='76 277 0' tangent='66 246 0' />
      <point pos='150 120 0' tangent='110 130 0' />
      <point pos='500 70 0' tangent='400 50 0' begin-tangent='190 80 0'/>
      <point pos='750 520 0' tangent='780 430 0' begin-tangent='650 60 0'/>
      <point pos='480 320 0' tangent='400 350 0' begin-tangent='700 560 0'/>
    </curve>
  </group>
  
  <!-- Particle test resources -->
  <group name='particle-test'>

    <texture type="2d" name="particle" path="images/particle.png" mag-filter="trilinear" min-filter="trilinear" />
    <material name='particle' src-blend='one' dst-blend='one' >
      <texture name='particle' />
    </material>

    <particle-affector name='point-affector-example' type='force' force='-50 0 0' />
    <particle-emitter name='point-emitter-example-1' type='point' material='particle'
                      life-span='-1.0'
                      max-particle-count='1500'
                      emission-rate='250'
                      particle-start-size='10 10' particle-start-size-variance='0 0' particle-end-size='10 10' particle-end-size-variance='0 0'
                      particle-life-span='5.0' particle-life-span-variance='0.0'
                      particle-start-color='0 0 0 1' particle-start-color-variance='0 0 0 0'
                      particle-end-color='1 1 1 1' particle-end-color-variance='0 0 0 0'
                      
                      emission-angle='45.0' emission-angle-variance='0.0'
                      emission-direction='1 0 0' emission-direction-mask='1 1 0'
                      particle-speed='50.0' particle-speed-variance='10.0'
                      emission-acceleration='0 10 0' emission-acceleration-variance='0 5 0'
                      >
      <affector name='point-affector-example' />
    </particle-emitter>

    <particle-emitter name='point-emitter-example-2' type='point' material='particle'
                      life-span='-1.0'
                      max-particle-count='3000'
                      emission-rate='300'
                      particle-start-size='10 10' particle-start-size-variance='0 0' particle-end-size='10 10' particle-end-size-variance='0 0'
                      particle-life-span='12.5' particle-life-span-variance='2.5'
                      particle-start-color='1 1 1 1' particle-start-color-variance='0 0 0 0'
                      particle-end-color='1 1 1 1' particle-end-color-variance='0 0 0 0'
                      
                      particle-start-position-variance='400 300 0'
                      emission-angle='45.0' emission-angle-variance='0.0'
                      emission-direction='1 0 0' emission-direction-mask='1 1 0'
                      particle-speed='0.0' particle-speed-variance='10.0'
                      emission-acceleration='0 10 0' emission-acceleration-variance='0 5 0'>
    </particle-emitter>
    
  </group>

  <!-- Localization test resources -->
  <group name='localization-test'>

    <text name='text-1' description='This is translator helping message. This is not interpreted by framework at all.'>
      <lang name='en' value='This is simple test' />
      <lang name='pl' value='To jest prosty test' />
    </text>

    <text name='text-2' description='Another translator helping message. This is not interpreted by framework at all.'>
      <lang name='en' value='This shows the number: %d' />
      <lang name='pl' value='To pokazuje liczbę: %d' />
    </text>

    <text name='text-3' description='Yet another translator helping message. This is not interpreted by framework at all.'>
      <lang name='en' value='There is %n apple.' />
      <lang name='en' value='There are %n apples.' />
      <lang name='pl' value='Tam jest %n jabłko.' />
      <lang name='pl' value='Tam są %n jabłka.' />
      <lang name='pl' value='Tam jest %n jabłek.' />
    </text>

  </group>

  <!-- Sound test resources -->
  <group name='sound-test'>

    <sound name='mono-22050' path='sounds/mono_22050.wav' />
    <sound name='stereo-22050' path='sounds/stereo_22050.wav' />
    <sound name='mono-11025' path='sounds/mono_11025.wav' />
    <sound name='stereo-11025' path='sounds/stereo_11025.wav' />

